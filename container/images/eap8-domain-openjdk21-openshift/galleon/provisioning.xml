<?xml version="1.0" ?>

<installation xmlns="urn:jboss:galleon:provisioning:3.0">
    <feature-pack location="org.jboss.eap:wildfly-ee-galleon-pack:">
    </feature-pack>
    <feature-pack location="org.jboss.eap.cloud:eap-cloud-galleon-pack:">
        <packages inherit="false">
            <include name="wildfly.s2i.common"/>
        </packages>
    </feature-pack>
    <feature-pack location="org.jboss.eap:eap-datasources-galleon-pack:">
        <packages inherit="false">
            <!-- The list of supported drivers : com.microsoft.sqlserver, com.oracle.database.jdbc, org.postgresql.jdbc -->
            <include name="org.postgresql.jdbc"/>
        </packages>
    </feature-pack>
</installation>
